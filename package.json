{
    "author": "Fernando G. Vilar",
    "bugs": {
        "url": "https://github.com/v08r/es-logging/issues"
    },
    "description": "Logging utils",
    "devDependencies": {
        "@commitlint/cli": "^17.1.2",
        "@commitlint/config-conventional": "^17.1.0",
        "@semantic-release/commit-analyzer": "^9.0.2",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/npm": "^9.0.1",
        "@semantic-release/release-notes-generator": "^10.0.3",
        "@types/jest": "^29.2.0",
        "@types/node": "^18.11.7",
        "@typescript-eslint/eslint-plugin": "^5.41.0",
        "@typescript-eslint/parser": "^5.41.0",
        "eslint": "^8.26.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^27.1.3",
        "eslint-plugin-simple-import-sort": "^8.0.0",
        "eslint-plugin-sort-keys-fix": "^1.1.2",
        "eslint-plugin-typescript-sort-keys": "^2.1.0",
        "husky": "^8.0.1",
        "jest": "^29.2.2",
        "jest-extended": "^3.1.0",
        "lint-staged": "^13.0.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "rimraf": "^3.0.2",
        "semantic-release": "^19.0.5",
        "ts-jest": "^29.0.3",
        "typedoc": "^0.23.19",
        "typescript": "^4.8.4"
    },
    "engines": {
        "node": ">=18.10.0",
        "pnpm": ">= 7.15.0"
    },
    "files": [
        "dist"
    ],
    "homepage": "https://github.com/v08r/es-logging#readme",
    "keywords": [
        "logging"
    ],
    "license": "MIT",
    "lint-staged": {
        "*.{ts}": [
            "prettier --write",
            "eslint --fix",
            "git add"
        ]
    },
    "main": "dist/main/index.js",
    "module": "dist/module/index.js",
    "name": "@vernacular/es-logging",
    "packageManager": "pnpm@7.15.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/v08r/es-logging.git"
    },
    "scripts": {
        "clean": "run-p clean:*",
        "clean:test:coverage": "rimraf coverage",
        "clean:build": "rimraf dist",
        "clean:docs": "rimraf docs/**/*.md",
        "prepublishOnly": "run-s clean format lint test build documentation",
        "documentation": "typedoc src/index.ts",
        "build:esm": "tsc",
        "build:cjs": "tsc -p ./tsconfig.cjs.json",
        "build": "run-p build:*",
        "format": "prettier --write ./src",
        "lint": "eslint --fix ./src/* --no-error-on-unmatched-pattern",
        "prepare": "husky install",
        "test": "jest",
        "test:coverage": "jest --coverage"
    },
    "types": "dist/module/index.d.ts",
    "version": "0.1.0"
}
